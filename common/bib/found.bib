@book{mortensen1995,
  author       = {Chris E. Mortensen},
  title        = {Inconsistent Mathematics},
  series       = {Mathematics and Its Applications},
  volume       = {312},
  year         = {1995},
  publisher    = {Springer},
  address      = {Dordrecht},
  isbn         = {0792331869},
