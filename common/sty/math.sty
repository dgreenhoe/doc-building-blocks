\newcommand{\formboxxyz}[4]{%formula box template
  \hspace{1pt}\newline$\ds{%
  \setlength{\arrayrulewidth}{1pt}%
  \arrayrulecolor{#1}%
  \begin{array}{|m{1.25ex}|l|}%
    \hline%
    \cellcolor{#1}\text{\fntHeros\color{white}\normalsize\upshape\scshape\bfseries#3}%
    &\displaystyle#4\\%
    \hline%
  \end{array}%
  \arrayrulecolor{arrayrulecolorNormal}%
  }$%
  }%
\newcommand{\formboxxyzp}[4]{%paragraph formula box template
  \hspace{1pt}\newline{%
  \setlength{\arrayrulewidth}{1pt}%
  \arrayrulecolor{#1}%
  \begin{tabular}{|m{1.25ex}|c|}%
    \hline%
    \cellcolor{#1}\text{\fntHeros\color{white}\normalsize\upshape\scshape\bfseries#3}%
    &\cellcolor{#2}%
     \begin{minipage}{\tw-4\arraycolsep-3\arrayrulewidth-1.25ex-1.5mm}#4\end{minipage}\\%
    \hline%
  \end{tabular}%
  \arrayrulecolor{arrayrulecolorNormal}%
  }%
  }%
