%--------------------------------------
% pst-node settings and support
% latmatall:
%   #   description           example
% ----------------------------------------------
%   1 = number of rows        {5}
%   2 = height                {20mm}
%   3 = height scaling factor {1}
%   4 = width  scaling factor {0.5}
%   5 = nodes                 {\null & \null ...}
%   6 = connecting lines      {\ncline{1,2}{2,1}\ncline{2,1}{3,3}...}
%   7 = node labels           {\nput{2,1}{$x$}}
%--------------------------------------
\newlength{\rowsepx}%   row separation variable
\newlength{\colsepx}%   column separation variable
\newlength{\nradius}%   node radius
\newlength{\ndiameter}% node diameter
\setlength{\nradius}{1.25mm}%
\setlength{\ndiameter}{2\nradius}%

\newcommand{\latmatall}[7]{%
  \setlength{\baselineskip}{2pt}%
  %\psset{linewidth=1.0pt}%
  \setlength{\rowsepx}{#2}
  \setlength{\rowsepx}{#3\rowsepx}
  \setlength{\rowsepx}{(\rowsepx-#1\ndiameter)/(#1-1)}
  %\setlength{\colsepx}{#2}
  %\setlength{\colsepx}{#3\colsepx}
  %\setlength{\colsepx}{(\colsepx-#1\ndiameter)/(#1-1)}
  %\setlength{\colsepx}{#4\colsepx}
  \setlength{\colsepx}{#4\rowsepx}
  \begin{psmatrix}[mnode=C, radius=\nradius, rowsep=\rowsepx, colsep=\colsepx, linecolor=latdot]%
    #5
  \end{psmatrix}%
  %\psset{linecolor=latline}%
    #6
  %\psset{labelsep=1pt}%
  \begin{color}{latlabel}%
    #7
  \end{color}%
  }%

\newcommand{\latmat}  [3]{\latmatall{#1}{17mm}{1} {1} {#2}{#3}{}  }% lattice matrix unlabeled
\newcommand{\latmatw} [4]{\latmatall{#1}{17mm}{1} {#2}{#3}{#4}{}  }% lattice matrix unlabeled with width scale factor
\newcommand{\latmath} [4]{\latmatall{#1}{17mm}{#2}{1} {#3}{#4}{}  }% lattice matrix unlabeled with height scale factor
\newcommand{\latmats} [5]{\latmatall{#1}{17mm}{#2}{#3}{#4}{#5}{}  }% lattice matrix unlabeled with height and width scale factors
\newcommand{\latmatl} [4]{% lattice matrix labeled
  \psset{unit=24mm}%
  \begin{pspicture}(-0.2,-0.2)(1.3,1.2)%
    \rput[bl](0,0){\latmatall{#1}{24mm}{1} {1} {#2}{#3}{#4}}%
    %\psgrid(-0.2,-0.2)(1.4,1.2)%
  \end{pspicture}%
  }%
\newcommand{\latmatlw}[5]{\latmatall{#1}{24mm}{1} {#2}{#3}{#4}{#5}}% lattice matrix labeled with width scale factor
\newcommand{\latmatlh}[5]{\latmatall{#1}{24mm}{#2}{1} {#3}{#4}{#5}}% lattice matrix labeled with height scale factor
\newcommand{\latmatls}[6]{\latmatall{#1}{24mm}{#2}{#3}{#4}{#5}{#6}}% lattice matrix labeled with height and width scale factors
  % 1 number of rows _________________|   |    |   |   |   |   |
  % 2 height (without scaling factor)_____|    |   |   |   |   |
  % 3 height scaling factor____________________|   |   |   |   |
  % 4 width scaling factor_________________________|   |   |   |
  % 5 nodes____________________________________________|   |   |
