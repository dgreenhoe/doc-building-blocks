@article{brown1991,
  author       = {Jason I. Brown and Stephen Watson},
  institution  = {Department of Mathematics and Statistics, York University},
  title        = {Self Complementary Topologies and Preorders},
  journal      = {Order},
  doi          = {10.1007/BF00383196},
  volume       = {7},
  number       = {4},
  year         = {1991},
  pages        = {317--328},
  issn         = {0167-8094 (print) 1572-9273 (online)},
  publisher    = {Springer Netherlands},
  url          = {http://www.springerlink.com/content/t164x9114754w4lq/},
  abstract     = {A topology on a set X is self complementary if
                  there is a homeomorphic copy on the same set that is a complement
                  in the lattice of topologies on X.
                  The problem of characterizing finite self complementary topologies
                  leads us to redefine the problem in terms of
                  preorders (i.e. reflexive, transitive relations).
                  A preorder P on a set X is self complementary
                  if there is an isomorphic copy P' of P on X that is arc disjoint
                  to P (except for loops) and with the property that P\cup P' is
                  strongly connected.
                  We characterize here self complementary finite partial orders
                  and self complementary finite equivalence relations.},
  }

